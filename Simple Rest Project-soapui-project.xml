 }

 List ExcelRead(String sheet)
 {
	
 	def emptyList = []
 	def groovyUtils = new com.eviware.soapui.support.GroovyUtils(context)
      def projectPath = groovyUtils.projectPath+"\\DATA.xls";
	Workbook workbook1 = Workbook.getWorkbook(new File(projectPath))
	Sheet sheet1 = workbook1.getSheet(sheet)
	def row=sheet1.getRows()
	log.info(row)
	log.info("-----")
	def column=sheet1.getColumns()
	log.info(column)
	for(int i=0;i&lt;column;i++)
	{
		if(i==column-1)
		{
			
			for(int j=2;j&lt;row;j++)
			{
				
				Cell a1=sheet1.getCell(i,j)
				
				//log.info(a1.getContents())
				emptyList.add(a1.getContents())
			}
		}
	}
	
	workbook1.close() 
	return emptyList
	 

}
